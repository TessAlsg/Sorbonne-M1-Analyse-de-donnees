      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy import stats"
      ],
      "metadata": {
        "id": "4iPln0H73Yac"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def plot_dirac(k0):\n",
        "    k = [k0 - 2, k0 - 1, k0, k0 + 1, k0 + 2]\n",
        "    pmf = [0, 0, 1, 0, 0] # PMF est 1 en k0\n",
        "\n",
        "    plt.figure(figsize=(8, 4))\n",
        "    plt.stem(k, pmf, basefmt=\" \", markerfmt=\"bo\", linefmt=\"b-\")\n",
        "    plt.title(f'Loi de Dirac en k0 = {k0}')\n",
        "    plt.xlabel('Valeur k')\n",
        "    plt.ylabel('P(X=k)')\n",
        "    plt.grid(axis='y', linestyle='--')\n",
        "    plt.show()\n",
        "\n",
        "# Test avec K0=5\n",
        "plot_dirac(k0=5)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 410
        },
