    {
      "cell_type": "code",
      "source": [
        "from scipy import stats\n",
        "import numpy as np\n",
        "\n",
        "# FONCTIONS DE BASE\n",
        "\n",
        "def calculer_moyenne(data):\n",
        "    \"\"\"\n",
        "    Calcule la moyenne arithmétique (espérance estimée) d'un ensemble de données.\n",
        "\n",
        "    Args:\n",
        "        data (list ou numpy.array): L'échantillon de données.\n",
        "\n",
        "    Returns:\n",
        "        float: La moyenne.\n",
        "    \"\"\"\n",
        "    if len(data) == 0:\n",
        "        return 0\n",
        "    # La somme des valeurs divisée par le nombre de valeurs\n",
        "    return np.sum(data) / len(data)\n",
        "\n",
        "def calculer_ecart_type(data, ddof=1):\n",
        "    \"\"\"\n",
        "    Calcule l'écart-type d'un ensemble de données (estimation de l'écart-type de la population).\n",
        "\n",
        "    Args:\n",
        "        data (list ou numpy.array): L'échantillon de données.\n",
        "        ddof (int): Degrés de liberté (Delta Degrees of Freedom).\n",
        "                    0 pour la population, 1 pour l'échantillon (std non biaisé).\n",
        "\n",
        "    Returns:\n",
        "        float: L'écart-type.\n",
        "    \"\"\"\n",
        "    if len(data) < 2:\n",
        "        # L'écart-type n'est pas défini pour moins de 2 points\n",
        "        return 0.0\n",
        "\n",
        "    mu = calculer_moyenne(data)\n",
        "\n",
        "    # Calcul de la variance (moyenne des carrés des écarts à la moyenne)\n",
        "    # Formule de la variance d'échantillon : Var = sum((x_i - mu)^2) / (N - ddof))\n",
        "    squared_differences = (data - mu)**2\n",
        "    variance = np.sum(squared_differences) / (len(data) - ddof)\n",
        "\n",
        "    # Calcul de l'écart-type\n",
        "    return np.sqrt(variance)\n",
        "\n",
        "\n",
        "# Test\n",
        "mu_poisson = 4.0\n",
        "N_samples = 10000\n",
        "samples_poisson = stats.poisson.rvs(mu_poisson, size=N_samples)\n",
        "\n",
        "# Calcul avec nos fonctions\n",
        "mean_calculated = calculer_moyenne(samples_poisson)\n",
        "std_calculated = calculer_ecart_type(samples_poisson, ddof=1)\n",
        "# Calcul (valeurs théoriques)\n",
        "# Loi de Poisson : Espérance E[X] = mu et Écart-type Std[X] = sqrt(mu)\n",
        "mean_theoretical = mu_poisson\n",
        "std_theoretical = np.sqrt(mu_poisson)\n",
        "\n",
        "print(f\"--- Loi de Poisson (mu={mu_poisson}) sur {N_samples} échantillons ---\")\n",
        "print(f\"Moyenne calculée : {mean_calculated:.4f} (Théorique : {mean_theoretical})\")\n",
        "print(f\"Écart-type calculé : {std_calculated:.4f} (Théorique : {std_theoretical:.4f})\")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "YRbCY0JZ4lH3",
        "outputId": "3f44ee64-0d3d-482f-fb91-56781b317e64"
      },
      "execution_count": 1,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "--- Loi de Poisson (mu=4.0) sur 10000 échantillons ---\n",
            "Moyenne calculée : 3.9999 (Théorique : 4.0)\n",
            "Écart-type calculé : 2.0011 (Théorique : 2.0000)\n"
          ]
