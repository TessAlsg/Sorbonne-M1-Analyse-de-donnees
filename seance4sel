      "cell_type": "code",
      "source": [
        "# Paramètres\n",
        "low = 1\n",
        "high = 7 # L'intervalle est [low, high-1]\n",
        "k = np.arange(low, high)\n",
        "\n",
        "# Calcul de la PMF\n",
