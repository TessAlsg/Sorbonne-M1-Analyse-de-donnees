        "    # pour une création simplifiée\n",
        "    colonnes_quantitatives.boxplot(column=colonne)\n",
        "\n",
        "    plt.title(f'Boîte à Moustache pour la colonne : {colonne}')\n",
        "    plt.ylabel('Valeur')\n",
        "    plt.grid(False) # Désactive la grille pour un rendu plus propre\n",
        "\n",
        "    # Sauvegarder la figure dans le dossier 'img'\n",
