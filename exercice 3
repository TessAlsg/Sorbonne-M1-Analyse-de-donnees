        "bins = [0, 10, 25, 50, 100, 2500, 5000, 10000, np.inf]\n",
        "\n",
        "# Étiquettes des catégories\n",
        "labels = [\n",
        "    ']0 - 10]', ']10 - 25]', ']25 - 50]', ']50 - 100]',\n",
        "    ']100 - 2500]', ']2500 - 5000]', ']5000 - 10000]', ']10000 + ['\n",
        "]\n",
        "\n",
        "# Création de la nouvelle colonne de catégories\n",
        "df_iles['Catégorie Surface'] = pd.cut(\n",
        "    df_iles['Surface (km²)'],\n",
        "    bins=bins,\n",
        "    labels=labels,\n",
        "    right=True,\n",
        "    include_lowest=True\n",
        ")\n",
        "\n",
        "# Dénombrement et affichage des résultats\n",
        "denouement = df_iles['Catégorie Surface'].value_counts().sort_index()\n",
        "print(denouement)"
